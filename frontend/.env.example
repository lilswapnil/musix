# Spotify API credentials
VITE_SPOTIFY_CLIENT_ID=your_spotify_client_id
VITE_SPOTIFY_REDIRECT_URI=https://yourdomain.com/callback
VITE_SPOTIFY_LOCAL_REDIRECT_URI=http://127.0.0.1:5173/callback
