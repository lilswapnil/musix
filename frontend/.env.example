# Tidal API credentials
VITE_TIDAL_CLIENT_ID=your_tidal_client_id_here  
VITE_TIDAL_CLIENT_SECRET=your_tidal_client_secret_here

# Genius API credentials (for lyrics)
VITE_GENIUS_CLIENT_ID=your_genius_client_id_here
VITE_GENIUS_ACCESS_TOKEN=your_genius_access_token_here
